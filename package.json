{
  "name": "nuxt-stub",
  "version": "1.0.0",
  "description": "My exceptional Nuxt.js project",
  "author": "yuichi",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server --watch api",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "seq:mgen": "cd database; sequelize model:generate",
    "seq:mig": "cd database; sequelize db:migrate",
    "seq:sgen": "cd database; sequelize seed:generate",
    "seq:seed": "cd database; sequelize db:seed:all",
    "seq:seedu": "cd database; sequelize db:seed:undo:all"
  },
  "dependencies": {
    "@nuxtjs/vuetify": "0.5.5",
    "cross-env": "^5.2.0",
    "express": "^4.17.3",
    "mysql2": "^1.6.5",
    "nuxt": "^2.0.0",
    "sequelize": "^5.9.4",
    "sequelize-cli": "^5.5.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "eslint": "^6.0.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-vue": "^5.2.3",
    "nodemon": "^1.18.9",
    "normalize.css": "^8.0.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2"
  }
}
